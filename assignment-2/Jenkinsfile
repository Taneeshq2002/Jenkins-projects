pipeline {
    agent any
    tools {
        maven 'Maven3.9.9'
    }
    environment {
        GIT_REPO = 'https://github.com/Taneeshq2002/Jenkins-projects.git'
    }
    stages {
        stage('Clone Repo') {
            steps {
                git branch: 'main', url: "${env.GIT_REPO}"
            }
        }

        stage('Build with Maven') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}
